<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="564.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.snl.Blue">
   <children>
      <ImageView fitHeight="700.0" fitWidth="564.0" pickOnBounds="true">
         <image>
            <Image url="@home2.jpg" />
         </image>
         <effect>
            <ColorAdjust brightness="-0.4" />
         </effect>
      </ImageView>
      <ImageView fx:id="menu1" fitHeight="559.0" fitWidth="400.0" layoutX="101.0" layoutY="52.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@colorbg.png" />
         </image>
      </ImageView>
      <ImageView fx:id="play1" fitHeight="160.0" fitWidth="133.0" layoutX="286.0" layoutY="537.0" onMouseClicked="#onPlay" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@play.png" />
         </image>
         <effect>
            <ColorAdjust brightness="0.3" contrast="0.3" />
         </effect>
      </ImageView>
      <ImageView fx:id="back1" fitHeight="64.0" fitWidth="228.0" layoutX="144.0" layoutY="535.0" onMouseClicked="#onBack" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@back.png" />
         </image>
         <effect>
            <ColorAdjust brightness="0.3" contrast="0.3" />
         </effect>
      </ImageView>
      <ImageView fitHeight="124.0" fitWidth="370.0" layoutX="129.0" layoutY="301.0" onMouseClicked="#onNext" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@next1.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
